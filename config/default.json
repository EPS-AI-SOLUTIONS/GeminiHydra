        "base": 1.05,
        "creativityBias": -0.08,
        "precisionBias": 0.12
      }
    },
    "eskel": {
      "role": "DevOps/Build",
      "model": "gemini-3-pro-preview",
      "temperatureProfile": {
        "base": 0.95,
        "creativityBias": -0.05,
        "precisionBias": 0.10
      }
    },
    "lambert": {
      "role": "Debugger",
      "model": "gemini-3-pro-preview",
