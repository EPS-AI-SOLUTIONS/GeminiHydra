{
  "$schema": "./schema.json",
  "version": "14.0.0",

  "models": {
    "gemini": {
      "pro": "gemini-3-pro-preview",
      "flash": "gemini-3-pro-preview"
    },
    "llama": {
      "default": "Qwen3-4B-Q4_K_M.gguf",
      "coder": "Qwen3-8B-Q4_K_M.gguf",
      "fast": "Qwen3-1.7B-Q4_K_M.gguf",
      "scout": "Qwen3-0.6B-Q4_K_M.gguf",
      "premium": "Qwen3-14B-Q4_K_M.gguf"
    },
    "default": "gemini-3-pro-preview",
    "fast": "gemini-3-pro-preview",
    "quality": "gemini-3-pro-preview",
    "local": "Qwen3-4B-Q4_K_M.gguf"
  },

  "temperatures": {
    "_comment": "Gemini 3 Pro: default=1.0, range=0.0-2.0, topP=0.95",
    "presets": {
      "deterministic": 0.7,
      "precise": 0.8,
      "focused": 0.9,
      "reasoning": 1.0,
      "balanced": 1.0,
      "moderate": 1.2,
      "creative": 1.5,
      "experimental": 1.8
    },
    "tasks": {
      "code_review": 0.9,
      "code_generation": 1.0,
      "security_scan": 0.8,
      "documentation": 1.0,
      "chain_of_thought": 1.0,
      "creative_writing": 1.5
    }
  },

  "limits": {
    "retry": {
      "maxRetries": 3,
      "maxCriticalRetries": 5,
      "initialDelayMs": 1000,
      "maxDelayMs": 60000,
      "backoffMultiplier": 2.0
    },
    "timeout": {
      "default": 120000,
      "fast": 30000,
      "long": 300000,
      "llama": 180000,
      "stream": 600000,
      "connection": 10000
    },
    "tokens": {
      "max": 8192,
      "maxFast": 4096,
      "maxLong": 16384,
      "maxContext": 128000,
      "reservedSystem": 2000,
      "safetyBuffer": 500
    },
    "concurrency": {
      "maxTasks": 12,
      "maxApiCalls": 6,
      "maxFileOps": 10,
      "maxParallelAgents": 8,
      "rateLimitRpm": 60,
      "rateLimitTpm": 100000
    },
    "cache": {
      "ttl": 3600000,
      "ttlShort": 60000,
      "ttlLong": 86400000,
      "maxSize": 1000,
      "maxSizeBytes": 104857600,
      "maxEntrySize": 1048576
    },
    "memory": {
      "maxEntries": 10000,
      "maxSessionHistory": 100,
      "maxConversationTurns": 50,
      "cleanupThreshold": 0.8
    },
    "file": {
      "maxSize": 10485760,
      "maxBatchFiles": 50,
      "maxDirectoryDepth": 10,
      "maxFileLines": 10000
    },
    "queue": {
      "maxSize": 100,
      "batchSize": 10,
      "drainTimeoutMs": 30000
    }
  },

  "agents": {
    "_comment": "Gemini 3 Pro optimized: default=1.0, safe min=0.7",
    "dijkstra": {
      "role": "Strategist",
      "model": "gemini:dynamic",
      "temperatureProfile": {
        "base": 0.95,
        "creativityBias": -0.05,
        "precisionBias": 0.10
      }
    },
    "geralt": {
      "role": "Security/VETO",
      "model": "gemini-3-pro-preview",
      "temperatureProfile": {
        "base": 0.85,
        "creativityBias": -0.10,
        "precisionBias": 0.15
      }
    },
    "yennefer": {
      "role": "Design patterns",
      "model": "gemini-3-pro-preview",
      "temperatureProfile": {
        "base": 0.90,
        "creativityBias": 0.05,
        "precisionBias": 0.05
      }
    },
    "triss": {
      "role": "QA/Testing",
      "model": "gemini-3-pro-preview",
      "temperatureProfile": {
        "base": 1.0,
        "creativityBias": 0.0,
        "precisionBias": 0.10
      }
    },
    "ciri": {
      "role": "Fast executor",
      "model": "gemini-3-pro-preview",
      "temperatureProfile": {
        "base": 1.15,
        "creativityBias": 0.10,
        "precisionBias": 0.0
      }
    },
    "jaskier": {
      "role": "User summaries",
      "model": "gemini-3-pro-preview",
      "temperatureProfile": {
        "base": 1.45,
        "creativityBias": 0.15,
