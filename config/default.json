        "base": 0.95,
        "creativityBias": 0.0,
        "precisionBias": 0.08
      }
    },
    "regis": {
      "role": "Researcher",
      "model": "gemini-3-pro-preview",
      "temperatureProfile": {
        "base": 1.1,
        "creativityBias": 0.05,
        "precisionBias": 0.05
      }
    },
    "philippa": {
      "role": "API specialist",
      "model": "gemini-3-pro-preview",
      "temperatureProfile": {
        "base": 0.90,
        "creativityBias": -0.05,
        "precisionBias": 0.10
      }
    }
  },

  "execution": {
    "adaptiveRetry": true,
    "partialCompletion": true,
    "parallelExecution": true,
    "autoDependencies": true,
    "checkpoints": true,
    "prioritization": true,
    "resourceScheduling": true,
    "gracefulDegradation": true,
    "templating": true,
    "profiling": true
  },

  "llama": {
    "modelsDir": "./data/models",
    "gpuLayers": 99,
    "contextSize": 8192,
    "batchSize": 512,
    "threads": 8,
    "flashAttention": true,
    "mmap": true,
    "mlock": false
  }
