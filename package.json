{
  "name": "claudehydra",
  "version": "2.1.0",
  "description": "ClaudeHydra - Unified CLI with Witcher Swarm (12 Agents)",
  "type": "module",
  "main": "src/server.js",
  "exports": {
    ".": "./src/server.js",
    "./cli": "./src/cli-unified/index.js",
    "./cli-unified": "./src/cli-unified/index.js",
    "./cli-unified/*": "./src/cli-unified/*.js"
  },
  "scripts": {
    "start": "node src/server.js",
    "cli": "node src/cli-unified/index.js",
    "cli:basic": "node src/cli-unified/index.js --mode basic",
    "cli:enhanced": "node src/cli-unified/index.js --mode enhanced",
    "cli:swarm": "node src/cli-unified/index.js --mode swarm",
    "hydra": "node src/cli-unified/index.js --mode swarm",
    "launcher": "node scripts/launcher.js",
    "bridge": "node scripts/bridge.js",
    "ai": "node scripts/ai-handler.js",
    "ai:query": "node scripts/ai-handler.js query",
    "ai:status": "node scripts/ai-handler.js status",
    "ai:list": "node scripts/ai-handler.js list",
    "swarm": "node src/cli-unified/index.js --mode swarm",
    "enhanced": "node src/cli-unified/index.js --mode enhanced",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:e2e": "playwright test",
    "verify": "node scripts/verify-integrity.js && npm run test && npm run test:e2e",
    "audit": "npm audit",
    "lint": "eslint .",
    "format": "prettier --check .",
    "format:write": "prettier --write .",
    "prepare": "husky install"
  },
  "bin": {
    "claudehydra": "src/cli-unified/index.js",
    "hydra-cli": "src/cli-unified/index.js"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.23.0",
    "ajv": "^8.17.1",
    "chalk": "^5.6.2",
    "cli-highlight": "^2.1.11",
    "dotenv": "^16.4.5",
    "envalid": "^8.0.0",
    "i18next": "^25.5.2",
    "marked": "^15.0.12",
    "marked-terminal": "^7.3.0",
    "node-notifier": "^10.0.1",
    "ora": "^8.2.0",
    "toml": "^3.0.0",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "eslint": "^9.0.0",
    "husky": "^9.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "vitest": "^4.0.18"
  },
  "engines": {
    "node": ">=20"
  }
}
