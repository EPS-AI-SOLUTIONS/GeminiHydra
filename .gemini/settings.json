{
  "security": {
    "auth": {
      "selectedType": "oauth-personal"
    }
  },
  "mcpServers": {
    "ollama": {
      "command": "cmd",
      "args": ["/c", "cd", "/d", "C:\\Users\\BIURODOM\\Desktop\\GeminiHydra", "&&", "npx", "tsx", "src/mcp/servers/OllamaMCPServer.ts"],
      "env": {
        "OLLAMA_HOST": "http://127.0.0.1:11434"
      },
      "timeout": 120000
    },
    "serena": {
      "command": "cmd",
      "args": ["/c", "uvx", "--from", "git+https://github.com/oraios/serena", "serena", "start-mcp-server", "--project-from-cwd"],
      "timeout": 120000
    },
    "desktop-commander": {
      "command": "cmd",
      "args": ["/c", "npx", "-y", "@wonderwhy-er/desktop-commander"],
      "env": {
        "DC_SHELL": "powershell"
      },
      "timeout": 60000
    },
    "playwright": {
      "command": "cmd",
      "args": ["/c", "npx", "-y", "@playwright/mcp@latest"],
      "timeout": 60000
    },
    "filesystem": {
      "command": "cmd",
      "args": ["/c", "cd mcp && node node_modules/@modelcontextprotocol/server-filesystem/dist/index.js . ../"],
      "timeout": 60000
    },
    "memory": {
      "command": "cmd",
      "args": ["/c", "cd mcp && node node_modules/@modelcontextprotocol/server-memory/dist/index.js"],
      "timeout": 60000
    },
    "sequential-thinking": {
      "command": "cmd",
      "args": ["/c", "npx", "-y", "@modelcontextprotocol/server-sequential-thinking"],
      "timeout": 60000
    },
    "context7": {
      "command": "cmd",
      "args": ["/c", "npx", "-y", "@upstash/context7-mcp"],
      "timeout": 60000
    }
  },
  "mcp": {
    "allowed": ["ollama", "serena", "desktop-commander", "playwright", "filesystem", "memory", "sequential-thinking", "context7"],
    "parallelToolCalls": true
  },
  "model": {
    "name": "gemini-3-pro-preview",
    "autoSelect": false,
    "preferences": {
      "code": "gemini-3-pro-preview",
      "simple": "gemini-3-pro-preview",
      "fast": "gemini-3-pro-preview",
      "default": "gemini-3-pro-preview"
    },
    "cachePrompts": true,
    "cacheTtl": 3600
  },
  "general": {
    "previewFeatures": true,
    "confirmTools": false,
    "maxRetries": 3,
    "retryDelay": 1000,
    "fallbacks": {
      "google": ["gemini-3-pro-preview", "gemini-3-pro-preview"]
    },
    "userPreferences": {
      "language": "pl",
      "useExternalSearch": true,
      "searchSources": ["google", "stackoverflow"]
    }
  },
  "context": {
    "fileName": ["GEMINI.md"]
  }
}