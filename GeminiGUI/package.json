    "@wdio/spec-reporter": "^9.23.3",
    "cross-env": "^10.1.0",
    "edgedriver": "^6.3.0",
    "jsdom": "^24.1.0",
    "lint-staged": "^16.2.7",
    "rimraf": "^5.0.5",
    "rollup-plugin-visualizer": "^6.0.5",
    "terser": "^5.46.0",
    "typescript": "5.8.3",
    "vite": "^7.3.1",
    "vite-plugin-compression": "^0.5.1",
